<!--Single Garage-->
<div id="garageTableHeader">
    {{sidebar.currentGarage.name}} Garage
</div>

<table id="garageTable">
    <tr>
        <td>Cost Per Month: </td>
        <td><!--{{sidebar.getCurrent().cost | currency}}--></td>
    </tr>
    <tr>
        <td>Address: </td>
        <td>{{sidebar.getCurrent().address}}</td>
    </tr>
    <tr>
        <td>Capacity: </td>
        <td>{{sidebar.getCurrent().capacity}}</td>
    </tr>
    <tr>
        <td>Leased Spaces: </td>
        <td>{{sidebar.getCurrent().numberOfLeasedSpaces}}</td>
    </tr>
    <tr>
        <td>Team-Member Spaces: </td>
        <td>{{sidebar.getCurrent().numberOfTeamMemberSpaces}}</td>
    </tr>
    <tr>
        <td>Required Buffer Size: </td>
        <td>{{sidebar.getCurrent().minimumNumberOfBufferSpaces}}</td>
    </tr>
    <tr>
        <td>Cost Per Space: </td>
        <td><!--{{sidebar.getCurrent().spaceCost | currency}}--></td>
    </tr>
    <tr>
        <td>Transient Sale Price: </td>
        <td>{{sidebar.getCurrent().transientSalePrice | currency}}</td>
    </tr>
    <tr>
        <td>Owner: </td>
        <td>{{sidebar.getCurrent().owner}}</td>
    </tr>
    <tr>
        <td>Billing Party: </td>
        <td>{{sidebar.getCurrent().billingParty}}</td>
    </tr>
    <tr>
        <td>Garage Manager ID: </td>
        <td>{{sidebar.getCurrent().garageManagerID}}</td>
    </tr>
</table>
