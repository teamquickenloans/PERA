<body ng-app="pera" ng-controller="TabController as tabCtrl">

<!--======================== Nav Bar ============================ -->
<nav class="top-bar" data-topbar role="navigation" data-options="is_hover:false">
    <!--TODO Make dropdown hoverable AND PROPERLY clickable -->
    <ul class="title-area">
        <li class="name">
            <h1><a href="#">Parking Dashboard</a></h1>
        </li>
        <li class="divider"></li>
    </ul>

    <section class="top-bar-section">

        <!--Right Nav Section -->
        <ul class="right" ui-view="right-nav">
        </ul>
                  
        <!-- Left Nav Section -->
        <ul class="left">
            <li class="divider"></li>
            <li class="has-dropdown" ui-sref-active="active">       <!--ng-class="{active: tabCtrl.isSet(3) || tabCtrl.isSet(4)}">-->
                <a href="#">Expenses</a>
                <ul class="dropdown">
                    <li ui-sref-active="active">                    <!--ng-class="{active: tabCtrl.isSet(3)}"-->
                        <a ui-sref="expense.discrepancies" ng-click="tabCtrl.setTab(3);">Identify Discrepancies</a>
                            </li>
                    <li ui-sref-active="active">                    <!--ng-class="{active: tabCtrl.isSet(4)}">-->
                        <a ui-sref="upload.uploadHistory" ng-click="tabCtrl.setTab(4);">Uploads</a>
                            </li>
                </ul>
            </li>
            <li class="divider"></li>
            <li class="has-dropdown" ui-sref-active="active">
                <!--ng-class="{active: tabCtrl.isSet(1) || tabCtrl.isSet(2)}">-->
                <a href="#">Garages</a>
                <ul class="dropdown" ng-controller="MapController as map">

                    <li ui-sref-active="active">
                        @*ng-class="{active: tabCtrl.isSet(1)}">*@
                        <a ui-sref="garage.map" ng-click="tabCtrl.setTab(1);">Garage Map</a>

                    </li>
                    <li ui-sref-active="active">
                        @*ng-class="{active: tabCtrl.isSet(2)}">*@
                        <a ui-sref="garage.edit" ng-click="tabCtrl.setTab(2);">Edit Garage</a>
                    </li>
                </ul>
                
            </li>
            <li class="divider"></li>
            <li ng-class="{active: tabCtrl.isSet(5)}">
                <a href="#" ng-click="tabCtrl.setTab(5);">Notifications</a>
            </li>
            <li class="divider"></li>
        </ul>
    </section>
</nav>

    <div class="column small-6 medium-8 large-10" id="map-canvas" ng-show="tabCtrl.isSet(1)"></div>
<div ui-view></div>

@*ng-show="$state.includes('garage.map')"*@


      <!-- Makes Foundation Stuff Work -->
    <script type="text/javascript">
        $(document).foundation();
    </script>
</body>

